# Django Environment Variables
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1,your-domain.railway.app

# Database (Railway will provide these automatically)
DATABASE_URL=postgresql://user:password@host:port/database

# Email Configuration
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password

# CORS Origins (Update with your frontend URL)
CORS_ALLOWED_ORIGINS=https://your-frontend.netlify.app,http://localhost:5174
